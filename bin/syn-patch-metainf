#!/usr/bin/env python

import sys
import json

dump = {}

try:
	f = sys.argv[1]
	fd = open(f)
	for line in fd:
		line = line.strip()
		if line == "":
			break
		idex = line.index(":")
		head = line[:idex].strip()
		data = line[idex+1:].strip()

		dump[head] = data
except IndexError as e:
	pass

print json.dumps(dump)
